<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="149.0"
      prefWidth="319.0" style="-fx-background-color: #000; -fx-border-color: #f98012;"
      stylesheets="@../css/albumList.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="net.etfbl.kdpo.client.AddImagesToAlbumWindowController">
    <children>
        <AnchorPane fx:id="anchorPane" prefHeight="29.0" prefWidth="320.0" style="-fx-background-color: #2c2c2c;">
            <children>
                <Label alignment="CENTER" layoutX="25.0" layoutY="27.0" text="Select Virtual album"
                       textAlignment="CENTER" textFill="#f98012" AnchorPane.bottomAnchor="5.0"
                       AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="5.0">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="165.0" prefWidth="185.0" VBox.vgrow="ALWAYS">
            <children>
                <ChoiceBox fx:id="albums" layoutX="14.0" layoutY="14.0" prefHeight="27.0" prefWidth="289.0"
                           style="-fx-background-color: #2c2c2c; -fx-border-color: #f98012; -fx-border-radius: 5px;"
                           styleClass="albumName"/>
                <Button fx:id="btnSave" focusTraversable="false" graphicTextGap="0.0" layoutX="40.0" layoutY="127.0"
                        mnemonicParsing="false" prefWidth="100.0" style="-fx-background-color: #f98012;" text="Save"
                        textFill="WHITE" AnchorPane.bottomAnchor="15.0"/>
                <Button fx:id="btnCancel" focusTraversable="false" graphicTextGap="0.0" layoutX="170.0" layoutY="127.0"
                        mnemonicParsing="false" prefWidth="100.0" style="-fx-background-color: #f98012;" text="Cancel"
                        textFill="WHITE" AnchorPane.bottomAnchor="15.0"/>
                <Label fx:id="lblErrorText" layoutX="20.0" layoutY="45.0" prefHeight="17.0" prefWidth="275.0"
                       text="Label" textFill="RED"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
