<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane minHeight="360.0" minWidth="640.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.etfbl.kdpo.client.MainController">
    <children>
        <HBox layoutX="200.0" layoutY="136.0" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <AnchorPane maxWidth="250.0" minWidth="250.0" prefWidth="250.0">
                    <children>
                        <TabPane id="tabPane" fx:id="tabPane" layoutX="57.0" layoutY="14.0" styleClass="tabPane"
                                 tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
                                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <tabs>
                                <Tab id="tabAlbumi" fx:id="tabAlbumi" closable="false" styleClass="tabAlbumi"
                                     text="Albumi">
                                    <content>
                                        <AnchorPane style="-fx-background-color: #2c2c2c;">
                                            <children>
                                                <Button id="buttonAddNewAlbum" fx:id="buttonAddNewAlbum"
                                                        contentDisplay="CENTER" focusTraversable="false"
                                                        maxHeight="35.0" minHeight="35.0" mnemonicParsing="false"
                                                        prefHeight="35.0" styleClass="buttonAddNewAlbum"
                                                        text="Add new album" AnchorPane.leftAnchor="3.0"
                                                        AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0"/>
                                                <ListView id="listView" fx:id="listView"
                                                          style="-fx-background-color: #2c2c2c;" styleClass="listView"
                                                          AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0"
                                                          AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="41.0"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                                <Tab id="tabFS" fx:id="tabFS" closable="false" styleClass="tabFS" text="File system">
                                    <content>
                                        <AnchorPane style="-fx-background-color: #2c2c2c;">
                                            <children>
                                                <TreeView fx:id="treeView" styleClass="treeView"
                                                          AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0"
                                                          AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                        <Label fx:id="lblMessages" alignment="CENTER" maxHeight="35.0" minHeight="35.0"
                               prefHeight="35.0"
                               style="-fx-background-color: #000000; -fx-text-fill: #ffffff; -fx-background-radius: 30;"
                               text="Messages" textAlignment="CENTER" AnchorPane.bottomAnchor="10.0"
                               AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0"/>
                    </children>
                </AnchorPane>
                <VBox minWidth="400.0" style="-fx-background-color: #000;" HBox.hgrow="ALWAYS">
                    <children>
                        <AnchorPane maxHeight="40.0" minHeight="40.0" prefHeight="40.0"
                                    style="-fx-background-color: #2c2c2c;">
                            <children>
                                <MenuButton fx:id="menu" focusTraversable="false" maxHeight="-Infinity"
                                            maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                            mnemonicParsing="false" prefHeight="40.0" prefWidth="60.0"
                                            styleClass="buttonMenu" textAlignment="CENTER" AnchorPane.bottomAnchor="1.0"
                                            AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="0.0">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="Action 1"/>
                                        <MenuItem mnemonicParsing="false" text="Action 2"/>
                                    </items>
                                </MenuButton>
                                <Label fx:id="labelAlbumDescription" layoutX="78.0" layoutY="12.0" minWidth="200.0"
                                       text="Label" textFill="WHITE" AnchorPane.bottomAnchor="0.0"
                                       AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="0.0">
                                    <font>
                                        <Font size="15.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttonAddNewImages" focusTraversable="false" layoutX="334.0"
                                        layoutY="8.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="60.0"
                                        styleClass="buttonAddNewImages" AnchorPane.bottomAnchor="1.0"
                                        AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0"/>
                                <Button fx:id="buttonSlideShow" focusTraversable="false" layoutX="291.0" layoutY="7.0"
                                        maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="60.0"
                                        styleClass="buttonSlideShow" AnchorPane.bottomAnchor="1.0"
                                        AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="0.0"
                                        onAction="#btnSlideShowOnMouseClickedMethod"/>
                                 </children>
                        </AnchorPane>
                        <ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false" hbarPolicy="NEVER"
                                    style="-fx-background-color: #000;" styleClass="scrollPane">
                            <VBox.margin>
                                <Insets left="5.0" top="5.0"/>
                            </VBox.margin>
                            <content>
                                <FlowPane fx:id="flowPane" hgap="5.0" style="-fx-background-color: #000;" vgap="5.0">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" top="5.0"/>
                                    </padding>
                                </FlowPane>
                            </content>
                        </ScrollPane>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
